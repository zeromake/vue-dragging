{
  "name": "awe-dnd",
  "version": "0.3.0",
  "description": "makes your elements draggable in Vue",
  "main": "vue-dragging.es5.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production webpack --progress --colors --config webpack.config.js",
    "dev": "cross-env NODE_ENV=development webpack --progress --colors --watch --config webpack.config.js",
    "karma-run": "karma run",
    "karma-start": "karma start test/unit/karma.conf.js",
    "karma-single": "karma start test/unit/karma.conf.js --single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hilongjw/vue-draging.git"
  },
  "keywords": [
    "vue-dragging",
    "vue-drag",
    "vue-dnd",
    "vue",
    "draggable",
    "drag",
    "drop",
    "html5",
    "draggable",
    "droppable",
    "drag-and-drop",
    "dnd"
  ],
  "author": "Awe <hilongjw@gmail.com>",
  "bugs": {
    "url": "https://github.com/hilongjw/vue-draging/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^3.5.0",
    "cross-env": "^4.0.0",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "ghooks": "^2.0.0",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "^2.0.0",
    "karma": "^1.6.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sinon-chai": "^1.3.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.3.0",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0",
    "validate-commit-msg": "^2.12.1",
    "vue": "^2.3.2",
    "vue-router": "^2.5.3",
    "webpack": "^2.4.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    }
  }
}
